{
  "name": "pagezero",
  "compatibility_date": "2025-08-23",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_compat_populate_process_env"
  ],
  "main": "workers/index.ts",
  "assets": {
    "directory": "./build/client",
    "binding": "ASSETS"
  },
  "preview_urls": true,
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true,
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "pagezero-development",
      "database_id": "<DATABASE_ID>"
    }
  ],
  "env": {
    "production": {
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "pagezero-production",
          "database_id": "<DATABASE_ID>"
        }
      ]
    },
    "preview": {
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "pagezero-preview",
          "database_id": "<DATABASE_ID>"
        }
      ]
    },
    "test": {
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "pagezero-test",
          "database_id": "<DATABASE_ID>"
        }
      ]
    }
  }
}
