{
  "name": "pagezero",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "react-router build && cp ./_headers ./build/client/_headers",
    "db:clean": "tsx ./db/clean.ts",
    "db:doctor": "drizzle-kit up && drizzle-kit check",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:init": "dotenvx run -f .dev.vars -- sh -c  'wrangler d1 execute $DB_NAME --local --file=\"./db/init.sql\"'",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:remove": "rm -rf ./.wrangler/state/v3/d1",
    "db:seed": "tsx ./db/seed.ts",
    "db:setup": "npm run db:remove && npm run db:init && npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:studio": "drizzle-kit studio",
    "dev": "NODE_OPTIONS=\"--inspect\" react-router dev",
    "doctor": "npm run format:check && npm run lint && npm run test:types && npm test",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "generate": "tsx ./scripts/generate/generate.ts",
    "git:clean": "git branch -l | grep -v 'main' | xargs git branch -D",
    "git:reset": "rm -rf .git && git init .",
    "lint": "eslint .",
    "setup": "npm install && cp .dev.vars.example .dev.vars && playwright install --with-deps chromium && npm run db:setup",
    "start": "wrangler pages dev ./build/client --port 3000",
    "storybook": "storybook dev -p 6006 --no-open",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --project=e2e",
    "test:e2e:ui": "playwright test --ui --project=e2e",
    "test:smoke": "playwright test --project=smoke",
    "test:watch": "vitest watch",
    "test:types": "tsc"
  },
  "dependencies": {
    "@heroicons/react": "2.1.3",
    "@mdx-js/react": "3.1.0",
    "@radix-ui/react-slot": "1.1.1",
    "@react-router/cloudflare": "7.1.5",
    "clsx": "2.1.1",
    "drizzle-orm": "0.39.2",
    "isbot": "5.1.21",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "7.1.5",
    "swr": "2.3.0"
  },
